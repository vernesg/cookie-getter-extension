{
  "manifest_version": 3,
  "name": "FB Cookie Extension",
  "version": "1.0",
  "description": "View Facebook cookies, UID, and profile info",
  "permissions": ["cookies", "tabs"],
  "host_permissions": ["https://*.facebook.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}