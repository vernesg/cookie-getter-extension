{
  "manifest_version": 3,
  "name": "Facebook Cookie Extractor",
  "version": "1.0",
  "description": "View Facebook cookies in JSON and string formats.",
  "permissions": ["cookies"],
  "host_permissions": ["*://*.facebook.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Facebook Cookie Extractor"
  }
}